{% block body %}
    <div class="container">        
        <table class="table">
            <th>Name</th>
            <th>Age</th>
            <th>Birthday</th>
            <th>Rank</th>
            {%for result in results%}
            <tr>
                <td><a href="/admin/student/{{result.id}}" title="{{result.name}}">{{result.name}}</a></td>           
                <td>{{result.age}}</td>            
                <td>{{result.birthday}}</td>            
                <td>{{result.rank}}</td>
            </tr>
            {%endfor%}
        </table>           
    </div>
{%endblock%}
